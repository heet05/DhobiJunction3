        <activity android:name=".ShareActivity" />
        <activity android:name=".ContacUsActivity" />
        <activity android:name=".MyOffersActivity" />
        <activity android:name=".VerficationActivity" />
        <activity android:name=".loginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".Navigation1Activity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".priceEstimator.PriceEstimatorActivity" />
